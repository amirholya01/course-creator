<!DOCTYPE html>
<html class="no-js">
<head>
    <meta charset="UTF-8">
    <title> Course Creator | @ViewData["Title"] </title>
    <partial name="_MetaTags" />
    <partial name="_Styles" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm" crossorigin="anonymous"></script>
    <link href="/assets/css/Default_Style.css" rel="stylesheet"/>
</head>
<body>
<div class="container-fluid" style="margin-top: 10px;">
        <nav class="navbar bg-dark">
            <div class="container-fluid">
                <a href="#" class="navbar-brand text-white"><img src="/assets/images/icons8-user.gif" alt="Animated GIF" /></a>
                <form class="d-flex" role="search">
                    <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                    <button class="btn btn-outline-success" type="submit">Search</button>
                </form>
            </div>
        </nav>
</div>
<div class="container-fluid" style="margin: 10px; margin-left:60px;">
    
            <div class="image-container">
                <img src="/assets/images/slide.jpg" alt="Slide">
                <div class="text-overlay">
                    <h2>Virtual Lab Course Creator</h2>
                </div>
            </div>

</div>

    <!-- main manu -->
    <partial name="_MainMenu" />
    <!-- /main manu -->
    <!-- content -->
    @RenderBody()
    <!-- /content -->
    <!-- footer -->
    <partial name="_Footer" />
    <!-- /footer -->

    <script src="/assets/js/bootstrap.min.js"></script>
    <script src="/assets/js/script.js"></script>
</body>
</html>
